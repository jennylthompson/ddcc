         fhir:Reference.reference [ fhir:value "ValueSet/who-svc-vaccines" ]
       ];
       fhir:ImplementationGuide.definition.resource.name [ fhir:value "WHO Vaccine List" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "The WHO Vaccine List from [ICD 11](https://icd.who.int/browse11) is used by:\n *  the [SVC Immunization](StructureDefinition-svc-immunization.html)" ]
     ], [
