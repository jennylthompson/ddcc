       ];
       fhir:ImplementationGuide.definition.resource.reference [
         fhir:link <http://fhir.org/guides/who/svc-rc1/StructureDefinition/svc-provenance-paper>;
         fhir:Reference.reference [ fhir:value "StructureDefinition/svc-provenance-paper" ]
       ];
       fhir:ImplementationGuide.definition.resource.name [ fhir:value "Provenance of a Paper SVC" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "The [Provenance of a Paper SVC](StructureDefinition-svc-provenance-paper.html) is used to assert the provenance of a Public Health Auhority (PHA) over a Paper SVC document at time of printing. A Paper SVC document may be a newly issued  or it may be a re-issue of an existing SVC document.\n\nWhen a new Paper SVC is printed, the following rules apply:\n * generate a [New SVC Patient](StructureDefinition-svc-patient-new.html) and a [New SVC Composition](StructureDefinition-svc-composition-new.html) resource \n * set the [agent.onBehalfOf](StructureDefinition-svc-provenance-paper-definitions.html#Provenance.agent.onBehalfOf) to reference the generated [New SVC Patient](StructureDefinition-svc-patient-new.html) resource \n * set the [agent.who]](StructureDefinition-svc-provenance-paper-definitions.html#Provenance.agent.who) to reference the Organization resource associated to the PHA via the WHO Smart Directory\n * set the [entity.what](StructureDefinition-svc-provenance-paper-definitions.html#Provenance.entity.what) to reference the generated [New SVC Composition](StructureDefinition-svc-composition-new.html) resource" ]
     ], [
       fhir:index 5;
       fhir:Element.extension [
         fhir:index 0;
         fhir:Extension.url [ fhir:value "http://hl7.org/fhir/tools/StructureDefinition/resource-information" ];
         fhir:Extension.valueString [ fhir:value "StructureDefinition:resource:abstract" ]
       ];
       fhir:ImplementationGuide.definition.resource.reference [
         fhir:link <http://fhir.org/guides/who/svc-rc1/StructureDefinition/svc-provenance>;
         fhir:Reference.reference [ fhir:value "StructureDefinition/svc-provenance" ]
       ];
       fhir:ImplementationGuide.definition.resource.name [ fhir:value "Provenance of SVC" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "Used to assert the provenance of a Public Health Auhority (PHA) over either a Paper or a Digital Smart Vaccination Card (SVC). This profile is not intended to be instantiated directly, rather a  [Provenance of an New Digital SVC](StructureDefinition-svc-provenance-digital-new.html), or a [Provenance of an Updated Digital SVC](StructureDefinition-svc-provenance-digital-updated.html), or a [Provenance of an Paper SVC](StructureDefinition-svc-provenance-paper.html) should be used." ]
     ], [
       fhir:index 6;
       fhir:Element.extension [
         fhir:index 0;
         fhir:Extension.url [ fhir:value "http://hl7.org/fhir/tools/StructureDefinition/resource-information" ];
         fhir:Extension.valueString [ fhir:value "StructureDefinition:resource" ]
       ];
       fhir:ImplementationGuide.definition.resource.reference [
         fhir:link <http://fhir.org/guides/who/svc-rc1/StructureDefinition/svc-bundle-new>;
         fhir:Reference.reference [ fhir:value "StructureDefinition/svc-bundle-new" ]
       ];
       fhir:ImplementationGuide.definition.resource.name [ fhir:value "New SVC Bundle" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "* amend this by transferring the non-digital demographic content  from the Paper SVC into the [SVC Patient] resource" ]
     ], [
       fhir:index 7;
       fhir:Element.extension [
         fhir:index 0;
         fhir:Extension.url [ fhir:value "http://hl7.org/fhir/tools/StructureDefinition/resource-information" ];
         fhir:Extension.valueString [ fhir:value "StructureDefinition:resource:abstract" ]
       ];
       fhir:ImplementationGuide.definition.resource.reference [
         fhir:link <http://fhir.org/guides/who/svc-rc1/StructureDefinition/svc-patient>;
         fhir:Reference.reference [ fhir:value "StructureDefinition/svc-patient" ]
       ];
       fhir:ImplementationGuide.definition.resource.name [ fhir:value "SVC Patient" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "Correponds to the demographic information on the front page of the Paper SVC. This profile is not intended to be instantiated directly, rather a [New SVC Patient](StructureDefinition-svc-patient-new.html) or an [Updated SVC Patient](StructureDefinition-svc-patient-updated.html) should be used.  \nA SVC_Patient should:\n* set the [identifier.assigner]((StructureDefinition-svc-patient-definitions.html#Patient.identifier.assigner) to reference the [SVC Organization](StructureDefinition-svc-organization.html) resource associated to the PHA via the WHO Smart Directory" ]
     ], [
       fhir:index 8;
       fhir:Element.extension [
         fhir:index 0;
         fhir:Extension.url [ fhir:value "http://hl7.org/fhir/tools/StructureDefinition/resource-information" ];
         fhir:Extension.valueString [ fhir:value "StructureDefinition:resource" ]
       ];
       fhir:ImplementationGuide.definition.resource.reference [
         fhir:link <http://fhir.org/guides/who/svc-rc1/StructureDefinition/svc-provenance-digital-updated>;
         fhir:Reference.reference [ fhir:value "StructureDefinition/svc-provenance-digital-updated" ]
       ];
       fhir:ImplementationGuide.definition.resource.name [ fhir:value "Provenance of an Updated Digital SVC" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "The Provenance of a Digital SVC is used to assert the provenance of a Public Health Auhority (PHA) over an updated Digital SVC document.  \n\nThe Provenance of an Updated Digital SVC should:\n * contain the [Updated SVC Patient](StructureDefinition-svc-patient-updated.html) demographic content tranferred from a Paper SVC\n * contain the [Updated SVC Immunization](StructureDefinition-svc-immunization.html) content tranferred from a Paper SVC \n * be generated by a PHA when a Paper SVC document is printed by a PHA, in which case:\n * set the [entity.what](StructureDefinition-svc-provenance-digital-updated-definitions.html#Provenance.entity.what) to reference the generated [Updated SVC Composition](StructureDefinition-svc-composition-updated.html) resource associated to Paper SVC\n * set the [role](StructureDefinition-svc-provenance-digital-updated-definitions.html#role)  to #derived" ]
     ], [
       fhir:index 9;
       fhir:Element.extension [
         fhir:index 0;
         fhir:Extension.url [ fhir:value "http://hl7.org/fhir/tools/StructureDefinition/resource-information" ];
         fhir:Extension.valueString [ fhir:value "StructureDefinition:resource" ]
       ];
       fhir:ImplementationGuide.definition.resource.reference [
         fhir:link <http://fhir.org/guides/who/svc-rc1/StructureDefinition/svc-immunization>;
         fhir:Reference.reference [ fhir:value "StructureDefinition/svc-immunization" ]
       ];
       fhir:ImplementationGuide.definition.resource.name [ fhir:value "SVC Immunization" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "An SVC Immunization contains the content to be encoded in a QR-code on a row in the Vaccination Events section of the Paper SVC. While this is not a further profile of the IPS Immunization (due to the requirement of gender/sex in IPS in the IPS Patient profile), this is intended to map onto an IPS Immunization." ]
     ], [
