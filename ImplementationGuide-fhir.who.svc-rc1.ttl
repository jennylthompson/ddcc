       fhir:ImplementationGuide.definition.resource.name [ fhir:value "SVC Bundle" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "An [SVC Bundle](StructureDefinition-svc-bundle.html) contains the content to be encoded in QR-code on the front page of a Paper SVC." ]
     ], [
       fhir:index 4;
       fhir:Element.extension [
         fhir:index 0;
