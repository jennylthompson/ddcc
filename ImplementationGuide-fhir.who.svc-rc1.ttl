       ];
       fhir:ImplementationGuide.definition.resource.reference [
         fhir:link <http://fhir.org/guides/who/svc-rc1/StructureDefinition/svc-composition-new>;
         fhir:Reference.reference [ fhir:value "StructureDefinition/svc-composition-new" ]
       ];
       fhir:ImplementationGuide.definition.resource.name [ fhir:value "New SVC Composition" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "Empty container for the health content contained on a new Paper SVC.\n\nWhen a new Paper SVC is issued, the following rules apply to a [New SVC Composition](StructureDefinition-svc-composition-new.html):\n  * a [Person.identifier](StructureDefinition-svc-person-definitions.html#Person.identifier)  entry is generated for a [SVC Patient](StructureDefinition-svc-patient.html) with system set to 'urn:uuid' and value set to a v4 UUID.\n  * [Composition.author](StructureDefinition-svc-composition-new-definitions.html#Composition.author) is set to reference the Organization resource associated to the PHA via the WHO Smart Directory\n  * set the [status](StructureDefinition-svc-composition-new-definitions.html#Composition.status) to #preliminary" ]
     ], [
       fhir:index 2;
       fhir:Element.extension [
         fhir:index 0;
